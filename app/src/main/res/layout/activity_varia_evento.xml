<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="br.com.ufpi.es.eventyacademic.es.ufpi.br.eventyacademic.visao.varia_evento">

    <Button
        android:id="@+id/buttonLogin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Login"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="4dp"
        android:onClick ="login" />

    <Button
        android:id="@+id/buttonEventos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Eventos"
        tools:layout_editor_absoluteX="92dp"
        tools:layout_editor_absoluteY="4dp"
        android:onClick="evento" />

    <Button
        android:id="@+id/buttonConsultarEventos"
        android:layout_width="295dp"
        android:layout_height="47dp"
        android:onClick="evento"
        android:text="Consultar Evento"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="117dp" />

    <Button
        android:id="@+id/buttonCadastrarEventos"
        android:layout_width="295dp"
        android:layout_height="47dp"
        android:onClick="evento"
        android:text="Cadastrar Evento"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="164dp" />





</android.support.constraint.ConstraintLayout>
